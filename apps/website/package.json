    "eslint-config-custom": "*",
    "postcss": "^8.4.30",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.3.1",
    "typescript": "^4.7.2",
    "vite": "^4.0.0"
  },
  "type": "module"
